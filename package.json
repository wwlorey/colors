{
	"name": "colors",
	"displayName": "Colors",
	"description": "A random theme generator that will make your eyes hurt.",
	"publisher": "wwlorey",
	"repository": {
		"type": "git",
		"url": "https://github.com/wwlorey/colors.git"
	},
	"version": "0.0.3",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.getRandomTheme",
		"onCommand:extension.removeRandomTheme",
		"onCommand:extension.togglePartyMode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.getRandomTheme",
				"title": "Get Random Theme",
				"category": "Colors"
			},
			{
				"command": "extension.removeRandomTheme",
				"title": "Remove Random Theme",
				"category": "Colors"
			},
			{
				"command": "extension.togglePartyMode",
				"title": "Toggle Party Mode",
				"category": "Colors"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
